public class LoginAuthenticationException extends RuntimeException{
	
	private static final long serialVersionUID = -3960328194907050648L;

	public LoginAuthenticationException(){
		super();
	}
	
	public LoginAuthenticationException(String message){
		super(message);
	}
	
	public LoginAuthenticationException(String message,Throwable casue){
		super(message,casue);
	}
}
